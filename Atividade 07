using System;

class Program
{
  public static void Main(string[] args)
  {
    Console.Write("Digite um número de ponto flutuante: ");
    float num1 = float.Parse(Console.ReadLine());
    Console.Write("Digite outro número de ponto flutuante: ");
    float num2 = float.Parse(Console.ReadLine());

    if (num1 <= num2)
    { Console.Write("O primeiro número é menor ou igual ao segundo");
    }
    else{
      Console.Write("O primeiro número não é menor ou igual ao segundo");
    }

  }
}
