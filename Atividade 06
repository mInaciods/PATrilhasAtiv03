using System;

class Program
{
  public static void Main(string[] args)
  {
    Console.Write("Digite um número de ponto flutuante: ");
    float num1 = float.Parse(Console.ReadLine());

    if (num1 % 2 == 0)
    { Console.Write("O número é par");
    }
    else{
      Console.Write("O número é ímpar");
    }

  }
}
